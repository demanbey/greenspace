<div>
<h1>Share your green space.</h1>
</div>
<div>
 <%= simple_form_for [@garden] do |f| %>

    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :capacity %>
    <%= f.input :location %>
    <%= f.input :size %>
    <%= f.input :category, collection: ["rooftop", "plot", "private", "vegetable only"] %>
    <%= f.input :photo %>
    <%= f.input :photo_cache, as: :hidden %>
  <div>
  <%= f.submit "lets get planting", class: "btn btn-primary" %>
  <% end %>
  </div>
</div>
